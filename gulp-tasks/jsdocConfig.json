{
  "tags": {
    "allowUnknownTags": true
  },
  "opts": {
    "destination": "./dist/docs",
    "template": "./node_modules/clean-jsdoc-theme",
    "theme_opts": {
        "default_theme": "dark",
        "homepageTitle": "Документация методов и классов проекта Dars"
    }
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true,
    "default": {
      "outputSourceFiles": true
    },
    "path": "./src",
    "navType": "vertical",
    "linenums": true,
    "dateFormat": "MMMM Do YYYY, h:mm:ss a"
  },
  "markdown": {
      "hardwrap": false,
      "idInHeadings": true
  }
}